{
  "name": "cellerity",
  "version": "0.1.2",
  "engines": {
    "node": ">=4.2.6"
  },
  "description": "Extensible general-purpose cellular automata",
  "keywords": [
    "cellular",
    "automata",
    "automaton",
    "Game of Life",
    "lifelike",
    "Conway",
    "procedural",
    "generation",
    "simulation"
  ],
  "main": "src/main.js",
  "scripts": {
    "documentation": "node_modules/.bin/jsdoc -c ./docs/conf.json",
    "build": "browserify src/main.js --standalone cellerity -o \"dist/cellerity-browser-\"$npm_package_version\".js\"",
    "test": "istanbul cover _mocha -- -R spec",
    "start": "echo \"Error: no start script specified\" && exit 1"
  },
  "directories": {
    "doc": "./docs/htmlDocs"
  },
  "author": {
    "name": "Avery Hiebert"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/averyhiebert/cellerity.git"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.3",
    "mocha": "^3.4.2"
  },
  "dependencies": {}
}
